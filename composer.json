{
    "name": "eltrino/orocrm-ebay-bundle",
    "description": "OroCRM eBay Integration Bundle",
    "homepage": "https://github.com/eltrino/EltrinoOroCRMEbayBundle",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Eltrino LLC",
            "homepage": "http://eltrino.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "Eltrino\\OroCrmEbayBundle": "."
        }
    },
    "repositories": [
      {
        "type": "git",
        "url": "https://github.com/orocrm/composer.git"
      },
      {
        "type": "composer",
        "url": "https://packagist.orocrm.com"
      }
    ],
    "target-dir": "Eltrino/OroCrmEbayBundle",
    "require": {
        "oro/crm": ">=2.0.0",
        "guzzle/guzzle": "3.7.*"
    },
    "require-dev": {
        "phpunit/phpunit":"~6.1",
        "eltrino/phpunit-mockannotations": "~2.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        }
    }
}
