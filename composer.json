    },
    "require-dev": {
        "eltrino/phpunit-mockannotations": "1.0.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
